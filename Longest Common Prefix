class Solution(object):
    def longestCommonPrefix(self, strs):
        if(strs==""):
            return s
        elif(1==len(strs)):
            return strs[0]
        else:
            strs=sorted(strs)
            s=""
            for i in range(len(strs[0])):
                if(strs[0][i]==strs[-1][i]):
                    s=s+strs[0][i]
                else:
                    break
        return s
